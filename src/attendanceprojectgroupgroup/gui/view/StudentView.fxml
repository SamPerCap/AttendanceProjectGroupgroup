<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="504.0" stylesheets="@CSS/CSS.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="attendanceprojectgroupgroup.gui.controller.StudentViewController">
    <children>
        <Label fx:id="idLabel" layoutX="44.0" layoutY="10.0" text="ID" AnchorPane.leftAnchor="44.0" AnchorPane.topAnchor="15.0">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <Label fx:id="nameLabel" layoutX="119.0" layoutY="10.0" text="Name" AnchorPane.leftAnchor="119.0" AnchorPane.topAnchor="15.0">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <ScrollPane layoutY="50.0" prefHeight="311.0" prefWidth="504.0">
            <content>
                <AnchorPane>
                    <children>
                        <TableView fx:id="weekTableView" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columns>
                                <TableColumn fx:id="columnWeek" minWidth="0.0" prefWidth="42.0" text="Week" />
                                <TableColumn fx:id="columnMonday" minWidth="0.0" prefWidth="75.0" text="Monday" />
                                <TableColumn fx:id="columnTuesday" minWidth="0.0" prefWidth="75.0" text="Tuesday" />
                                <TableColumn fx:id="columnWednesday" minWidth="0.0" prefWidth="75.0" text="Wednesday" />
                                <TableColumn fx:id="columnThursday" minWidth="0.0" prefWidth="75.0" text="Thursday" />
                                <TableColumn fx:id="columnFriday" minWidth="0.0" prefWidth="75.0" text="Friday" />
                                <TableColumn fx:id="columnWeekTotal" minWidth="0.0" prefWidth="70.0" text="WeekTotal" />
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
      <Button fx:id="btnSeeDetailsChart" layoutX="401.0" layoutY="367.0" mnemonicParsing="false" onAction="#clickSeeDetailsChart" text="See details chart" AnchorPane.bottomAnchor="8.0" AnchorPane.rightAnchor="0.0" />
    </children>
</AnchorPane>
